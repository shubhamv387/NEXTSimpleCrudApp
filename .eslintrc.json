{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-alert": "off"
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [["@", "./app"]],
        "extensions": [".js", ".jsx", ".json"]
      }
    }
  }
}
